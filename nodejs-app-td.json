{
    "family": "nodejs-app-task-definition",
    "containerDefinitions": [
        {
            "name": "nodejs",
            "image": "docker.io/marieswaran/nodejs-app:v_%BUILD_NUMBER%",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "cpu": 256,
            "memory": 512
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ]
}

