{
    "family": "nodejs-app-task-definition",
    "containerDefinitions": [
        {
            "name": "nodejs",
            "image": "docker.io/marieswaran/nodejs-app:v_%BUILD_NUMBER%",
            "portMappings": [
                {
                    "name": "nodejs-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "cpu": "256",
            "memory": "512"
        }
    ]
}

